    {% set recentDate = groupResult|last.completeDraw.drawDate %}
    {% if key|length == 9 %}
    {% set countBalls = key|slice(6, 1) %}
    {% set countStars = key|slice(8, 1) %}
    <div
        class="flex justify-center items-center border-b-2 border-neutral-100 px-6 py-3 dark:border-neutral-600 dark:text-neutral-50"
    >
        {{ resultsCount }} tirages gagnants pour {{ countBalls }}
        <div class="table_icon-ball-euromillions"></div> + {{ countStars }}
        <div class="table_icon-star"></div>
    </div>
    <div class="p-6">
        <h5
            class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
            Stats pour chaque numéros
